%b Alcool :
= alcool.name

%b | Quantity :
= alcool.quantity

= form_for alcool do |f|
  .field
    = f.label :decrease
    = f.text_field "temp", "size" => 3 
  .field
    = f.submit
= form_for alcool do |f|
  .field
    = f.label :increase
    = f.text_field "temp", "size" => 3
  .field
    = f.submit
%br
